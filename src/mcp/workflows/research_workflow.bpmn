<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             targetNamespace="http://brainforge.ai/research"
             id="Definitions_1">
  
  <process id="ResearchWorkflow" name="BrainForge Research Workflow" isExecutable="true">
    
    <!-- Start Event -->
    <startEvent id="StartEvent_1" name="Start Research">
      <outgoing>SequenceFlow_1</outgoing>
    </startEvent>
    
    <!-- Topic Analysis Task -->
    <userTask id="UserTask_1" name="Topic Analysis">
      <documentation>Analyze the research topic and define scope</documentation>
      <incoming>SequenceFlow_1</incoming>
      <outgoing>SequenceFlow_2</outgoing>
    </userTask>
    
    <!-- Source Discovery Task -->
    <userTask id="UserTask_2" name="Source Discovery">
      <documentation>Discover relevant sources for research</documentation>
      <incoming>SequenceFlow_2</incoming>
      <outgoing>SequenceFlow_3</outgoing>
    </userTask>
    
    <!-- Content Analysis Task -->
    <userTask id="UserTask_3" name="Content Analysis">
      <documentation>Analyze gathered content</documentation>
      <incoming>SequenceFlow_3</incoming>
      <outgoing>SequenceFlow_4</outgoing>
    </userTask>
    
    <!-- Synthesis Task -->
    <userTask id="UserTask_4" name="Synthesis">
      <documentation>Synthesize findings into coherent insights</documentation>
      <incoming>SequenceFlow_4</incoming>
      <outgoing>SequenceFlow_5</outgoing>
    </userTask>
    
    <!-- Report Generation Task -->
    <userTask id="UserTask_5" name="Report Generation">
      <documentation>Generate final research report</documentation>
      <incoming>SequenceFlow_5</incoming>
      <outgoing>SequenceFlow_6</outgoing>
    </userTask>
    
    <!-- End Event -->
    <endEvent id="EndEvent_1" name="Research Complete">
      <incoming>SequenceFlow_6</incoming>
    </endEvent>
    
    <!-- Sequence Flows -->
    <sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_1" targetRef="UserTask_1"/>
    <sequenceFlow id="SequenceFlow_2" sourceRef="UserTask_1" targetRef="UserTask_2"/>
    <sequenceFlow id="SequenceFlow_3" sourceRef="UserTask_2" targetRef="UserTask_3"/>
    <sequenceFlow id="SequenceFlow_4" sourceRef="UserTask_3" targetRef="UserTask_4"/>
    <sequenceFlow id="SequenceFlow_5" sourceRef="UserTask_4" targetRef="UserTask_5"/>
    <sequenceFlow id="SequenceFlow_6" sourceRef="UserTask_5" targetRef="EndEvent_1"/>
    
  </process>
  
  <!-- BPMN Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="ResearchWorkflow">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="173" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_1_di" bpmnElement="UserTask_1">
        <dc:Bounds x="250" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_2_di" bpmnElement="UserTask_2">
        <dc:Bounds x="400" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_3_di" bpmnElement="UserTask_3">
        <dc:Bounds x="550" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_4_di" bpmnElement="UserTask_4">
        <dc:Bounds x="700" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_5_di" bpmnElement="UserTask_5">
        <dc:Bounds x="850" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="1000" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="SequenceFlow_1_di" bpmnElement="SequenceFlow_1">
        <di:waypoint x="209" y="120"/>
        <di:waypoint x="250" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_2_di" bpmnElement="SequenceFlow_2">
        <di:waypoint x="350" y="120"/>
        <di:waypoint x="400" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_3_di" bpmnElement="SequenceFlow_3">
        <di:waypoint x="500" y="120"/>
        <di:waypoint x="550" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_4_di" bpmnElement="SequenceFlow_4">
        <di:waypoint x="650" y="120"/>
        <di:waypoint x="700" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_5_di" bpmnElement="SequenceFlow_5">
        <di:waypoint x="800" y="120"/>
        <di:waypoint x="850" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_6_di" bpmnElement="SequenceFlow_6">
        <di:waypoint x="950" y="120"/>
        <di:waypoint x="1000" y="120"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>